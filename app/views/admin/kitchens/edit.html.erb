<h2>商品編集</h2>
<%= form_with model: @kitchen, url: admin_kitchen_path(@kitchen.id), method: :PATCH do |f| %>
<p>商品画像</p>
<%= f.file_field :image, accept: "image/*" %>
<p>商品名</p>
<%= f.text_field :name %>
<p>商品説明</p>
<%= f.text_area :introduction %>
<p>税抜価格</p>
<%= f.text_field :price %> 円
<%= f.submit "変更を保存", class:"btn btn-success" %>
<% end %>

<%= link_to '削除', admin_kitchen_path, method: :delete %>